     "data": {
      "text/plain": [
       "array([[ 4.,  5., 16., 20., 25.]])"
      ]
     },
     "execution_count": 79,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "poly = PolynomialFeatures(include_bias=False)\n",
    "# 편향(절편)값을 0으로 둠\n",
    "# degree(차수) 지정 가능. default는 2. 2제곱만큼이 특성으로 계산됨\n",
    "\n",
    "poly.fit([[4, 5]])\n",
    "# 특성을 만들어줄 것을 입력 -> .fit()으로 계산\n",
    "poly.transform([[4, 5]])"
   ]
  },
